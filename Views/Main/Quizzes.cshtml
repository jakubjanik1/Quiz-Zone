@using Quiz_Zone.Models
@model IEnumerable<string>

@{
    Layout = "~/Views/_Layout.cshtml";
}

<!DOCTYPE html>
<html>
<head>
    <title>Quizzes</title>
</head>
<body>
    @foreach (var category in Model)
    {
        @Html.ActionLink(category, "Play", new { category = category })
    }
</body>
</html>
